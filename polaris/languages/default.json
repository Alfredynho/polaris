                "help": "Type %shelp to know all I can do!"
            }
        },
        "cats": {
            "commands": {
                "cat": {
                    "command": "/cat",
                    "friendly": "Cat",
                    "parameters": {}
                }
            },
            "description": "Sends random photos of cats."
        },
        "pins": {
            "commands": {
                "pin": {
                    "command": "/pin ",
                    "friendly": "Pin as ",
                    "parameters": {
                        "name": true
                    }
                },
                "unpin": {
                    "command": "/unpin",
                    "friendly": "Unpin",
                    "parameters": {
                        "name": true
                    }
                },
                "pins": {
                    "command": "/pins",
                    "friendly": "Pin list",
                    "parameters": {}
                }
            },
            "description": "Easy commands.",
            "strings": {
                "pins": "<b>Pins</b>:",
                "pinned": "<b>#%s</b> was added.",
                "unpinned": "<b>#%s</b> was deleted.",
                "already_pinned": "<b>#%s</b> exists, cannot add.",
                "not_found": "<b>#%s</b> doesn't exist.",
                "not_creator": "<b>#%s</b> cannot be deleted, you are not the creator."
            }
        },
        "reminders": {
            "commands": {
                "remindme": {
                    "command": "/remindme",
                    "friendly": "^Remindme to ",
                    "parameters": {
                        "delay": true,
                        "text": true
                    }
                }
            },
            "description": "Set a reminder for yourself.",
            "strings": {
                "wrongdelay": "The delay must be in this format: '(integer)(s|m|h|d)'.\nExample: '2h' for 2 hours.",
                "noreminder": "Please include a reminder.",
                "added": "<b>%s</b>, I'll remind you in <b>%s</b> to <i>%s</i>."
            }
        },
